<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.watson.mapper.OrderDetailMapper">

    <resultMap id="BaseResultMap" type="com.watson.entity.OrderDetailEntity">
